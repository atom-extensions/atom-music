{
  "name": "atom-music",
  "main": "./lib/atom-music",
  "version": "2.0.3",
  "description": "A very simple music player for listening to your favorite music while working in Atom.",
  "keywords": [
    "music",
    "audio",
    "player",
    "sound"
  ],
  "activationCommands": {
    "atom-workspace": [
      "atom-music:toggle",
      "atom-music:play-pause",
      "atom-music:toggle-shuffle",
      "atom-music:search-playlist",
      "atom-music:forward",
      "atom-music:rewind",
      "atom-music:next-track",
      "atom-music:previous-track"
    ]
  },
  "deserializers": {
    "atom-music/AtomMusicView": "deserializeAtomMusicView"
  },
  "repository": "https://github.com/sdinesh86/atom-music",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "atom --test spec",
    "lint": "eslint ."
  },
  "dependencies": {
    "atom-select-list": "^0.7.2",
    "etch": "^0.14.0"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^8.0.0",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "semantic-release": "^21.0.1",
    "lodash": "^4.17.21",
    "dot-prop": "^4.2.1",
    "temp": "^0.9.1"
  }
}
